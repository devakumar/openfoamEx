FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 1000;

boundaryField
{
    front_face
    {
        type            empty;
    }
    back_face
    {
        type            empty;
    }
    potato_surface
    {
        type            fixedValue;
        value           uniform 113;
    }
    potato_center
    {
        type            fixedValue;
        value           uniform 113;
    }
    oven_boundary
    {
        type            fixedValue;
        value           uniform 3;
    }
    defaultFaces
    {
        type            fixedValue;
        value           uniform 113;
    }
}


